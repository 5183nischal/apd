    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Attach",
            "type": "debugpy",
            "request": "attach",
            "connect": {
                "host": "localhost",
                "port": 5678
            },
        },
        {
            "name": "tms_lp",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/spd/experiments/tms/tms_decomposition.py",
            "args": "${workspaceFolder}/spd/experiments/tms/tms_lp_config.yaml",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYDEVD_DISABLE_FILE_VALIDATION": "1"
            }
        },
        {
            "name": "tms_topk",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/spd/experiments/tms/tms_decomposition.py",
            "args": "${workspaceFolder}/spd/experiments/tms/tms_topk_config.yaml",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYDEVD_DISABLE_FILE_VALIDATION": "1"
            }
        },
        {
            "name": "resid_mlp_topk",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/spd/experiments/resid_mlp/resid_mlp_decomposition.py",
